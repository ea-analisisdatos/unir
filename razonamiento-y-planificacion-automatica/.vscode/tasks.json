{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Ejecutar LiSAT",
      "type": "shell",
      "command": "./lisat",
      "args": [
        "-d",
        "${workspaceFolder}/domain.pddl",
        "-p",
        "${workspaceFolder}/problem.pddl",
        "-l",
        "50"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
      },
      "problemMatcher": []
    }
  ]
}
